{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [0, "always"],
    "semi": [0, "never"],
    "space-before-function-paren": [0, "never"],
    "indent": [0, "never"],
    "jsx-a11y/anchor-is-valid": "off",
    "no-script-url": "off",
    "camelcase": [
      "error",
      {
        "properties": "always"
      }
    ]
  }
}
