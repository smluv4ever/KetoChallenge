{
  "name": "sabio-web-api",
  "version": "1.0.0",
  "description": "Express based Node Api",
  "engines": {
    "node": ">=10.14.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node .compiled/server.js",
    "compile": "concurrently \"npm:babel-app\" \"npm:notify\" ",
    "kill-node-debugger": "PowerShell.exe \"%CD%\\killSomeNode.ps1\" ",
    "kill-node": "PowerShell.exe \"%CD%\\killNode.ps1\" ",
    "~~~~~~~~ pre commit ~~~~~~~~": "",
    "lint": "eslint --color -f util/eslintFormatter.js . ",
    "babel-app": "babel app --out-dir .compiled --source-maps --watch --delete-dir-on-start",
    "~~~~~~~~ utilityies below ~~~~~~~~": "",
    "notify": "node util/watch.js",
    "lint-fix": "eslint --fix --color -f util/eslintFormatter.js . ",
    "postinstall": "npm run link-cmd",
    "link-cmd": "PowerShell.exe \"%CD%\\link.ps1\" ",
    "spinup-die": "node shortlife.js",
    "check-integrity": "del-cli .compiled && babel app --out-dir .compiled --delete-dir-on-start && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "http://code.sabio.la:8080/tfs/SabioCollection/"
  },
  "author": "Sabio - Gregorio Rojas",
  "license": "ISC",
  "displayName": "SabioApi",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "chokidar": "^2.0.4",
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "extend": "^3.0.2",
    "fs": "0.0.1-security",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "reflect-metadata": "^0.1.12",
    "require-all": "^3.0.0",
    "socket.io": "^2.2.0",
    "tedious-connection-pool": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "babel-code-frame": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-watch": "git+https://git@github.com/kmagiera/babel-watch.git",
    "del-cli": "^1.1.0",
    "eslint": "^5.12.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "install": "^0.12.2",
    "npm-run-all": "^4.1.5"
  }
}
